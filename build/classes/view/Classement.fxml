<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ClassementController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Classement">
         <font>
            <Font name="Impact" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="table_classement" layoutX="56.0" layoutY="91.0" prefHeight="239.0" prefWidth="488.0">
        <columns>
          <TableColumn fx:id="rang" prefWidth="36.0" text="Rang" />
          <TableColumn fx:id="logo" prefWidth="43.0" text="Logo" />
            <TableColumn fx:id="team" prefWidth="179.0" text="Team" />
            <TableColumn fx:id="play" prefWidth="45.0" text="P" />
            <TableColumn fx:id="win" prefWidth="46.0" text="W" />
            <TableColumn fx:id="lost" prefWidth="46.0" text="L" />
            <TableColumn fx:id="diff" prefWidth="46.0" text="Diff" />
            <TableColumn fx:id="pts" prefWidth="46.0" text="PTS" />
        </columns>
      </TableView>
      <Label fx:id="ftCompetition" layoutX="121.0" layoutY="14.0" text="Competition" textFill="#f80202">
         <font>
            <Font name="Impact" size="20.0" />
         </font>
      </Label>
      <Label fx:id="ftPhase" layoutX="245.0" layoutY="14.0" text="Phase" textFill="#868080">
         <font>
            <Font name="Impact" size="20.0" />
         </font>
      </Label>
      <Label layoutX="114.0" layoutY="4.0" text=":">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label layoutX="231.0" layoutY="3.0" text=":">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Button fx:id="btDashboard" layoutX="544.0" layoutY="14.0" mnemonicParsing="false" onAction="#Back" text="Back" />
      <Button fx:id="btPDF" layoutX="492.0" layoutY="14.0" mnemonicParsing="false" onAction="#SavePDF" text="PDF" />
   </children>
</AnchorPane>
